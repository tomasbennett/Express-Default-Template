<section>
    <% if (messages?.length > 0) { %>

        <ul>

            <% for (let i = 0; i < messages.length; i++) { %>
    
                <li>
                    <p>
                        <%= messages[i].username %>
                    </p>
                </li>
    
            <% } %>

        </ul>

    <% } else { %>

        <p class="no-messages">
            There are no messages currently.
        </p>


    <% } %>
</section>



<%- contentFor('pageTitle') %>
<title>Home Page</title>


